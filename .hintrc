{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": "off",
    "css-prefix-order": "off",
    "no-inline-styles": "off",
    "typescript-config/consistent-casing": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-active": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-progressbar-name": "off",
        "aria-valid-attr-value": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 123",
    "not firefox <= 123",
    "not ios_saf <= 17.4",
    "not safari <= 17.4"
  ]
}